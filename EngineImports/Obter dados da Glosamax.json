            "metadata": {
                "designer": {
                    "x": 600,
                    "y": 150
                },
                "parameters": [
                    {
                        "name": "handleErrors",
                        "type": "boolean",
                        "label": "Evaluate all states as errors (except for 2xx and 3xx )",
                        "required": true
                    }
                ],
                "expect": [
                    {
                        "name": "url",
                        "type": "url",
                        "label": "URL",
                        "required": true
                    },
                    {
                        "name": "method",
                        "type": "hidden",
                        "label": "Method"
                    },
                    {
                        "name": "shareCookies",
                        "type": "boolean",
                        "label": "Share cookies with other HTTP modules",
                        "required": true
                    }
                ]
            },
            "onerror": [
                {
                    "id": 14,
                    "module": "postgres:InsertIntoTable",
                    "version": 2,
                    "parameters": {
                        "table": "\"public\".\"log_importacoes\"",
                        "account": 1050177
                    },
                    "mapper": {
                        "@id": "",
                        "@data": "{{now}}",
                        "@link": "{{7.baseUrl}}{{7.linkdownload}}",
                        "@source": "glosamax-getfile",
                        "@status": "",
