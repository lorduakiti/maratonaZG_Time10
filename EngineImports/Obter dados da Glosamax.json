                        "@deleted_at": "",
                        "@lote_importacao": "{{12.defaultDatasetId}}",
                        "@referencia_documento": "{{7.linkname}}"
                    },
                    "metadata": {
                        "designer": {
                            "x": 900,
                            "y": 300
                        },
                        "restore": {
                            "table": {
                                "label": "public.log_importacoes"
                            },
                            "account": {
                                "label": "My PostgreSQL Time 10"
                            }
                        },
                        "parameters": [
                            {
                                "name": "account",
                                "type": "account",
                                "label": "Connection",
                                "required": true
                            },
                            {
                                "name": "table",
                                "type": "select",
                                "label": "Table",
                                "required": true
                            }
                        ],
                        "expect": [
                            {
                                "name": "@id",
                                "type": "integer",
                                "label": "id"
