                            {
                                "name": "@data",
                                "type": "text",
                                "label": "data"
                            },
                            {
                                "name": "@referencia_documento",
                                "type": "text",
                                "label": "referencia_documento"
                            },
                            {
                                "name": "@link",
                                "type": "text",
                                "label": "link"
                            },
                            {
                                "name": "@created_at",
                                "type": "text",
                                "label": "created_at"
                            },
                            {
                                "name": "@deleted_at",
                                "type": "text",
                                "label": "deleted_at"
                            },
                            {
                                "name": "@status",
